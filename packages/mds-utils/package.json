{
  "name": "mds-utils",
  "version": "0.1.3",
  "private": true,
  "description": "Mobility Data Specification utility functions",
  "keywords": [
    "mds",
    "utils"
  ],
  "author": "City of Los Angeles",
  "license": "Apache-2.0",
  "dependencies": {
    "circle-to-polygon": "1.0.2",
    "point-in-polygon": "1.0.1"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc --build .",
    "test": "yarn test:prettier && yarn test:eslint && yarn test:unit",
    "test:eslint": "eslint --ignore-path ../../.gitignore '**/*.ts'",
    "test:prettier": "prettier --write --check --ignore-path ../../.gitignore '**/*.ts'",
    "test:unit": "nyc --check-coverage --exclude tests --extension .ts --lines 5 --reporter=text --reporter=html ts-mocha --require source-map-support/register --recursive tests/**/*.ts"
  }
}
