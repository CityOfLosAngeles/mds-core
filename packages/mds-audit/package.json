{
  "name": "mds-audit",
  "version": "0.1.13",
  "private": true,
  "description": "Endpoint for auditing compliance with MDS",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc --build .",
    "start": "yarn watch server",
    "test": "yarn test:prettier && yarn test:eslint && yarn test:unit",
    "test:eslint": "eslint --ignore-path ../../.gitignore '**/*.ts'",
    "test:prettier": "prettier --write --check --ignore-path ../../.gitignore '**/*.ts'",
    "test:unit": "nyc --check-coverage --exclude tests --extension .ts --lines 90 --reporter=text --reporter=html ts-mocha --require source-map-support/register --recursive tests/**/*.ts",
    "watch": "nodemon --watch '../../packages' --ext 'ts' --ignore '*.d.ts' --exec yarn watch:exec --",
    "watch:exec": "yarn build && ts-node"
  },
  "keywords": [
    "mds",
    "audit"
  ],
  "author": "City of Los Angeles",
  "dependencies": {
    "@hapi/joi": "15.1.0",
    "express": "4.17.1",
    "mds-api-authorizer": "^0.1.1",
    "mds-api-helpers": "^0.1.1",
    "mds-api-server": "^0.1.1",
    "mds-cache": "^0.1.1",
    "mds-db": "^0.1.1",
    "mds-enums": "^0.1.1",
    "mds-logger": "^0.1.1",
    "mds-providers": "^0.1.1",
    "mds-utils": "^0.1.1",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "mds-test-data": "^0.1.1"
  },
  "license": "Apache-2.0"
}
