{
  "name": "@aws-lambda/aws-serverless-express-handler",
  "version": "0.1.2",
  "private": true,
  "description": "Handler for AWS Serverless Express (AWS Lambda)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc --build .",
    "test": "yarn test:prettier && yarn test:eslint && yarn test:unit",
    "test:eslint": "eslint --ignore-path ../../.gitignore '**/*.ts'",
    "test:prettier": "prettier --write --check --ignore-path ../../.gitignore '**/*.ts'",
    "test:unit": "nyc --check-coverage --exclude tests --extension .ts --lines 90 --reporter=text --reporter=html ts-mocha --require source-map-support/register --recursive tests/**/*.ts"
  },
  "keywords": [
    "mds"
  ],
  "author": "City of Los Angeles",
  "dependencies": {
    "@aws-lambda/api-gateway-authorizer": "0.1.2",
    "@aws-lambda/aws-utils": "0.1.2",
    "aws-serverless-express": "3.3.6",
    "mds-api-server": "0.1.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.27",
    "@types/aws-serverless-express": "3.3.1"
  },
  "license": "Apache-2.0"
}
