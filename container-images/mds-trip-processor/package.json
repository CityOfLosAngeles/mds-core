{
  "name": "@container-images/trip-processor",
  "version": "0.1.0",
  "description": "",
  "main": "dist/index.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "bundle": "yarn build && webpack --mode=production --env.npm_package_name=${npm_package_name} --env.npm_package_version=${npm_package_version}",
	"image": "../../bin/image-build mds-trip-processor:${npm_package_version}",
    "start": "node index.js",
    "pretest": "eslint --ignore-path .gitignore --ignore-pattern static/*.js .",
    "test": "nyc --check-coverage --lines 70 --per-file mocha --recursive tests"
  },
  "nyc": {
    "exclude": [
      "util/*",
      "index.js"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@mds-core/mds-types": "0.1.6",
